# Configuración del servidor
PORT=:8420
READ_TIMEOUT=15s
WRITE_TIMEOUT=15s
IDLE_TIMEOUT=60s
SHUTDOWN_TIMEOUT=15s

# Configuración de WebSocket
READ_BUFFER_SIZE=1024
WRITE_BUFFER_SIZE=1024

# Clave AES para encriptación (requerida - debe tener 32 caracteres)
# IMPORTANTE: Cambia esta clave por una segura en producción
# Puedes generar una clave segura con: openssl rand -hex 16
CHAT_AES_KEY=tu-clave-segura-de-32-caracteres-aqui

# Configuración de desarrollo (opcional)
DEBUG=true
LOG_LEVEL=info 